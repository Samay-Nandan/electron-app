{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "bin": "dist/server.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/server.ts",
    "build": "tsc -p . && tsc-alias",
    "pkg:linux": "pkg . --targets node18-linux-x64 --output $BUILD_OUTPUT/server/server",
    "pkg:mac": "pkg . --targets node18-macos-x64 --output $BUILD_OUTPUT/server/server",
    "pkg:win": "pkg . --targets node18-win-x64 --output $BUILD_OUTPUT/server/server.exe",
    "start": "node -r tsconfig-paths/register dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "pkg": "^5.8.1",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.28.0"
  }
}
